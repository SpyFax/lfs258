[control_plane]
${ public_ip } ansible_user=${ admin_user }

[worker]
%{ for value in k8s_hostnames ~}
${ value.id } ansible_host=${ value.public_ip}
%{ endfor ~}

[ha_control_plane]
%{ for value in control_plane ~}
${ value.id } ansible_host=${ value.public_ip}
%{ endfor ~}

[all:vars]
master_ip=${ master_ip}
